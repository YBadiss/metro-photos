    "@typescript-eslint/parser": "^8.51.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.6.2",
    "prettier": "^3.7.4",
